    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-modal": "^3.16.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/toastify-js": "^1.12.0",
    "babel-loader": "^9.1.3",
